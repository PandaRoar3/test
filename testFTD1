local function autoUpgrade(npcs)
    local RunService = game:GetService("RunService")
    local Timer = 0
    local TimerMax = 2

    RunService.Heartbeat:Connect(function(Delta)
        Timer += Delta
        if Timer >= TimerMax then
            Timer = 0
            for i, npc in ipairs(npcs) do
                local args = {
                    [1] = workspace.PlayerFodel.Npc[npc]
                }
                game:GetService("Players").LocalPlayer.PlayerGui.UpLevel.Frame.Uplevel.LocalScript.RemoteEvent:FireServer(unpack(args))
            end
        end
    end)
end

MainSection:NewButton("Auto Upgrade", "Makes you do gymnastics", function()
    local npcsToUpgrade = {"Eren", "Erwin", "Enji", "Kizaru"}
    autoUpgrade(npcsToUpgrade)
end)
MainSection:NewButton("Place Imu", "spawn Imu", function()
    placeUnit("Marine", game.Players.LocalPlayer)
    wait(1)
    sellUnit(workspace.Units.Marine)
    wait(1)
    placeUnit("Imu", game.Players.LocalPlayer)
end)
MainSection:NewButton("Teleport to Room", "Teleport to Room", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(
-3606.81812, 1039.22803, -1108.78674, -0.906289101, 0.0733914673, -0.416237652, 2.10808544e-08, 0.984808743, 0.173642471, 0.422658354, 0.157370269, -0.892521441)
end)
