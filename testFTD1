            [1] = "Eren",
            [2] = CFrame.new(21.9669, 10.492, -64.734)
        }
        game:GetService("ReplicatedStorage").Event.TowerOn:FireServer(unpack(towerArgs))
        local towerArgs = {
            [1] = "Erwin",
            [2] = CFrame.new(21.966, 10.492, -68.734)
        }
        game:GetService("ReplicatedStorage").Event.TowerOn:FireServer(unpack(towerArgs))
        local towerArgs = {
            [1] = "Enji",--1
            [2] = CFrame.new(14.244, 2.055, -68.496)
        }
        game:GetService("ReplicatedStorage").Event.TowerOn:FireServer(unpack(towerArgs))
        local towerArgs = {
            [1] = "Enji",--2
            [2] = CFrame.new(14.244, 2.055, -70.496)
        }
        game:GetService("ReplicatedStorage").Event.TowerOn:FireServer(unpack(towerArgs))
        local towerArgs = {
            [1] = "Enji",--3
